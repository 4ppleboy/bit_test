{
  "require": {
    "altorouter/altorouter": "1.2.0",
    "atlas/orm": "2.0.0",
    "doctrine/annotations": "^1.5",
    "php-di/php-di": "5.4.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Safely/constants.php",
      "app/Safely/config.php",
      "app/Safely/bootstrap.php"
    ],
    "classmap": [
      "app/App.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Tests\\Fixtures\\": "tests/fixtures/"
    }
  },
  "config": {
    "platform": {
      "php": "7.1"
    },
    "sort-packages": true
  }
}
